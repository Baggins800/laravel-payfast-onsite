{
    "name": "fintech-systems/laravel-payfast",
    "description": "This is my package laravel-payfast",
    "keywords": [
        "FintechSystems",
        "Laravel",
        "PayFast"
    ],
    "homepage": "https://github.com/fintech-systems/laravel-payfast",
    "license": "MIT",
    "authors": [
        {
            "name": "Eugene van der Merwe",
            "email": "eugenevdm@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "FintechSystems\\Payfast\\": "src/"            
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "FintechSystems\\Payfast\\Tests\\": "tests"
        }
    },    
    "require": {
        "php": "^8.0",
        "symfony/dotenv": "^5.3",
        "illuminate/contracts": "^8.37",
        "payfast/payfast-php-sdk": "^1.1",
        "spatie/ray": "^1.32",
        "livewire/livewire": "^2.8",
        "moneyphp/money": "^4.0"
    },
    "require-dev": {  
        "nunomaduro/collision": "^5.3",      
        "orchestra/testbench": "^6.20",          
        "phpunit/phpunit": "^8.0|^9.0",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "FintechSystems\\Payfast\\PayfastServiceProvider"
            ],
            "aliases": {
                "VirtualminApi": "FintechSystems\\Payfast\\Facades\\Payfast"
            }
        }
    }
}
